{
    "manifest_version": 2,
    "name": "MDN",
    "version": "1.0",
    "description": "an Chrome extension to help you stay focused!",
    "content_scripts": [
        {
          "matches": ["https://www.google.com/*"],
          "js": ["./Data/mdnSitemap.js", "./main.js"],
          "css": ["style.css"]
        }
      ],
    "browser_action": {
        "default_popup": "https://www.google.com"  
      }
}